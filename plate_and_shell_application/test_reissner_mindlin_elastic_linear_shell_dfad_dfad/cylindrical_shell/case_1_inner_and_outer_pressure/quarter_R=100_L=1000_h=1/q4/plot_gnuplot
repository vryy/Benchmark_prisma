#set terminal postscript eps enhanced color
set terminal pdf
set view equal xy
set size ratio -1
#set size 1.5, 1.5

#set output "convergence_error.eps"
set output "convergence_error.pdf"
set xlabel "log10 - (#dofs)^{1/2}"
set ylabel "log10 - error norm"
set xtics 0.5
set ytics 1.0
set title "Convergence of error norm"
plot "convergence.log" using (log10($4)/2):(log10($5)) with linespoints title "q4-l2", \
