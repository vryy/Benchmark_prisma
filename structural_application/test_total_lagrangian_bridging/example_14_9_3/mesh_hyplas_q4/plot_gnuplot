#set size square
set size ratio 5./6.
set key right bottom

set output "monitoring.pdf"
set xlabel "Displacement (mm)"
set ylabel "Reaction force (kN)"
set xtics 0,1,6
set ytics 0,2,10
#set title ""
set xrange[0:6]
set yrange[0:10]
plot "monitoring.log" using ($1):(2*$2) with lines title "Von-Mises (Total Lagrangian)", \
